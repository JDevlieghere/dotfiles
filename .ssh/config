Host *
    IgnoreUnknown AddKeysToAgent,UseKeychain
    AddKeysToAgent yes
    UseKeychain yes
    KeepAlive yes
    ForwardAgent yes
    ServerAliveInterval 60
    ServerAliveCountMax 2

Host localhost
	UserKnownHostsFile /dev/null
	StrictHostKeyChecking no

Include ~/.ssh/greendragon
Include ~/.ssh/bluedragon
